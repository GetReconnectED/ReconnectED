        "nuxt": "^4.2.1",
        "nuxt-vuefire": "^1.1.0",
        "typescript": "^5.6.3",
        "vue": "^3.5.24",
        "vue-router": "^4.6.3"
    },
    "devDependencies": {
        "@iconify-json/lucide": "^1.2.74",
        "@nuxt/test-utils": "^3.20.1",
        "@vitest/coverage-v8": "^4.0.8",
        "@vue/test-utils": "^2.4.6",
        "firebase-admin": "^13.6.0",
        "happy-dom": "^20.0.10",
        "playwright-core": "^1.56.1",
