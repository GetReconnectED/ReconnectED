{
    "name": "reconnected-web",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "deploy": "nuxt build && wrangler deploy",
        "deploy:nobuild": "wrangler deploy",
